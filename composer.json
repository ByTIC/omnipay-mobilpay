{
  "name": "bytic/omnipay-mobilpay",
  "type": "library",
  "license": "MIT",
  "description": "Mobilpay driver for ThePhpLeague Omnipay multi-payment processing library",
  "keywords": [
    "mobilpay",
    "driver",
    "gateway",
    "merchant",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/ByTIC/omnipay-mobilpay",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Omnipay\\Mobilpay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Omnipay\\Mobilpay\\Tests\\": "tests/src",
      "ByTIC\\Omnipay\\Mobilpay\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "ext-dom": "*",
    "bytic/omnipay-common": "^1.0"
  },
  "suggest": {
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2|^7.0|^8.0|^9.0",
    "vlucas/phpdotenv": "^2.3",
    "mockery/mockery": "^1.0"
  }
}
